<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Travel angency</title>
    <link rel="stylesheet" href="/stylesheets/style.css">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH" crossorigin="anonymous">
</head>
<body>
  <% allbooks.forEach(function(data,index){ %>
    <div class="footer-right"><h2 class="bg-dark">Contact</h2>
        <form  class=" bg-secondary p-5" action="/create" method="post">
          <input type="username" placeholder="Enter the name" name="user" value="<%=data.user%>">
          <input type="email" placeholder="Enter the email" name="email" value="<%=data.email%>">
          <input type="number" placeholder="Enter your number" name="number" value="<%=data.number%>">
          <button>Read</button>
        </form>
        </div>
        <%})%>
</body>
</html>